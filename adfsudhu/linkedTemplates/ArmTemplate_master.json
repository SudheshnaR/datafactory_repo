{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfsudhu"
		},
		"ls_AzureSqlDatabasesudhu_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_AzureSqlDatabasesudhu'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=cloudsqlsudhu.database.windows.net;Initial Catalog=cloud_databasesudhu;User ID=cloud"
		},
		"ls_SqlServer_sudhu_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_SqlServer_sudhu'"
		},
		"ls_blobstoragesudhu_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blobstoragesudhu'"
		},
		"ls_datalake_sudhu_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_datalake_sudhu'"
		},
		"ls_datalakesudhu_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_datalakesudhu'"
		},
		"ls_mysql_sudhu_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_mysql_sudhu'"
		},
		"AzureKeyVaultsudhu_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://keyvaultsudhu.vault.azure.net/"
		},
		"ls_datalake_sudhu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakesudhu.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfsudhu_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_blobstoragesudhu_connectionString": {
						"value": "[parameters('ls_blobstoragesudhu_connectionString')]"
					},
					"ls_datalake_sudhu_accountKey": {
						"value": "[parameters('ls_datalake_sudhu_accountKey')]"
					},
					"ls_datalakesudhu_connectionString": {
						"value": "[parameters('ls_datalakesudhu_connectionString')]"
					},
					"AzureKeyVaultsudhu_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVaultsudhu_properties_typeProperties_baseUrl')]"
					},
					"ls_datalake_sudhu_properties_typeProperties_url": {
						"value": "[parameters('ls_datalake_sudhu_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adfsudhu_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_AzureSqlDatabasesudhu_connectionString": {
						"value": "[parameters('ls_AzureSqlDatabasesudhu_connectionString')]"
					},
					"ls_SqlServer_sudhu_connectionString": {
						"value": "[parameters('ls_SqlServer_sudhu_connectionString')]"
					},
					"ls_mysql_sudhu_connectionString": {
						"value": "[parameters('ls_mysql_sudhu_connectionString')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adfsudhu_ArmTemplate_0')]"
			]
		},
		{
			"name": "adfsudhu_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adfsudhu_ArmTemplate_1')]"
			]
		}
	]
}